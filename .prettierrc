{
  "semi": true,
  "printWidth": 100,
  "singleQuote": false,
  "useTabs": false,
  "tabWidth": 2,
  "quoteProps": "preserve",
  "trailingComma": "none",
  "jsxBracketSameLine": true,
  "bracketSameLine": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "css",
  "vueIndentScriptAndStyle": true,
  "embeddedLanguageFormatting": "auto",
  "proseWrap": "preserve"
}

